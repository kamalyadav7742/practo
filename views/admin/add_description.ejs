                            <div class="row justify-content-evenly mt-3">
                              <div class="col-md-4">
                                <label for="name">speciality</label>
                              </div>
                              <div class="col-md-8">
                                <input type="text" id="speciality" name="speciality" maxlength="18"
                                  placeholder="Enter Specialist">
                              </div>
                            </div>


                            <div class="row my-4">
                              <div class="row justify-content-evenly">
                                <a href="/add_description"> <button type="submit" id="form_submit"
                                    class="btn btn-primary">Add Profile</button></a>
                              </div>
                            </div>
                      </form>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <!-- end -->

          <!-- start labtest -->
